package java_package;

public class reverse_number {
	
	public static void main(String[] args) {
		
		int a = 123;
		int original_number;
		
		int rev;
		int q;
		for(int i=1;i<=a;i++) {
			
			rem =a%10; // 3 
			
			q=a/10; // 12
			
			original_number=rem*10+rem;
			
			
		}
		System.out.println(rev);
		 
		 
		
		
		
		
	}

}
