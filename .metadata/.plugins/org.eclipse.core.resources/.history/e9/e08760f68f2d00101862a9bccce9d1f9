package java_package;

import java.util.Scanner;

public class prime_number {
	
	public static void main(String[] args) { 
		
		Scanner sc =new Scanner(System.in);
		int a = sc.nextInt();
		
		boolean isprime = false;
		
		for(int i=2; i<=a*a;i++) {
			
			if(a%i==0) {
				isprime = true;
				break;
			}
		}

		
		
	}

}
