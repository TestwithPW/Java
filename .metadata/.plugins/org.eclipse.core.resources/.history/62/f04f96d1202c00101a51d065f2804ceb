package java_package;

public class infinite_for_loop {
	
	public static void main(String[] args) {
		 int a =10;
		 
		 int b= 20;
		 
		 int c = a*b;
		 
		 
		for(int i=1; i<5 ; i++) {
			
 
			 for(int j=1;j<5;j++) {
				 
				 for(int k=1;k<5;k++) {
				 
				 System.out.println(i +" - " + j + " - " + k );
				 }
			 }
			
	 	}

	}

}
