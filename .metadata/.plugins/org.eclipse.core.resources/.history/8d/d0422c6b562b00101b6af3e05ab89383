package java_package;

public class loops_in_java {
	
	public static void main(String[] args) {
		 int a =10;
		 
		 int b= 20;
		 
		 int c = a*b;
		 
		 for(int i=1; i<= 10 ; i++) {
			 
			 if (i%2==0)
			 {
			 
			 System.out.println("c value is " + c + " - " i );
			 
		 }
		 }
		
		
		
	}

}
