package java_package;

public class swap_numbers_without_using_third_variable {
	
	public static void main(String[] args) { // table in incremental way 


		// swap the number 
				int p=66; int q= 89;
				
				p= p +q; // 10+20 = 10
				q= p -q; // 30-20 = 20
				p= p -q; // 30-10 = 10
				System.out.println("value of q :" +q);
				System.out.println("value of p :" +p);
		
	}

}
