package java_package;

public class reverse_number {
	
	public static void main(String[] args) {
		
		int a = 123;
		int original_number;
		
		int rev =0;
		
		for(int i=1;i<=a;i++) {
			
			rev =a%10; 
			
			rem=a/10;
			
			rev=rev*10+rem;
			
			
		}
		System.out.println(rev);
		 
		 
		
		
		
		
	}

}
