package Statements_keywords_in_java;

public class continue_keyword_do_while_loop {
	
	public static void main(String[] args) {
		
		
			int i =1 ;
			System.out.println("Value of i : " + i);
			do {
				
				i++;
				if(i==4) {
					
					continue;	
				}
				
				if (i==5) {
					break;
				}
				System.out.println("Value of i : " + i);
				
				
				
			}
			while(i<=5);
			
			System.out.println("First loop is over");
			System.out.println(" ");
			
			
			// or 
			do {
				i++;
				if(i==4) {
					
					continue;	
				}
				System.out.println("Value of i : " + i);
				i++;
				
				
			}
			while(i<=5);
			
	
			
	}

}
